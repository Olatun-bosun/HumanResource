
<div class="row my-3">
    <label class="form-label col-4">@Label</label>
    <div class="col-8">
        <span class="form-control">@Text</span>
    </div>
</div>

@code {
    [Parameter, EditorRequired] public string? Label { get; set; }
    [Parameter, EditorRequired] public string? Text { get; set; }
}
